---
const PROJECTS = [
  {
    title: "",
    description: "",
    link: "",
    github: "",
    img: "",
    tags: [],
  },
];
---

{
  PROJECTS.map(({ title, description, link, github, img, tags }) => (
    <article>
      <h3>{title}</h3>
      <p>{description}</p>
      <ul>
        {tags.map((tag) => (
          <li>{tag}</li>
        ))}
      </ul>
      <img src={img} alt=`Screenshot about ${img}` />
    </article>
  ))
}
